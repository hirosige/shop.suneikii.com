<div class="page-title">
  <div class="title_left">
    <h4><%= render_breadcrumbs builder: ::CustomBreadcrumbsBuilder %></h4>
  </div>

  <div class="title_right">
    <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
      <div class="input-group">
        <input type="text" class="form-control" placeholder="<%= t('admin.elements.search.keyword') %>">
        <span class="input-group-btn">
              <button class="btn btn-default" type="button"><%= t('admin.elements.search.button') %></button>
            </span>
      </div>
    </div>
  </div>
</div>